    <dubbo:reference id="brokerTradeConsumer" interface="com.zendaimoney.thirdpp.trade.pub.service.IBrokerTradeService"  check="false"  retries="0" timeout="70000"/>
    <dubbo:reference id="baseServiceConsumer" interface="com.zendaimoney.thirdpp.trade.pub.service.IBaseService"  check="false"  retries="0" timeout="70000"/>
    <dubbo:reference id="authServiceConsumer" interface="com.zendaimoney.thirdpp.trade.pub.service.IAuthService"  check="false"  retries="0" timeout="70000"/>
</beans>                                                 